{
  "name": "codio-sui-maker",
  "displayName": "Codio-SUI-Maker",
  "description": "Record and playback the process of programming. Fork from Codio-SUI which is fork from Codio.",
  "repository": {
    "url": "https://github.com/karina-celis/codio-sui-maker",
    "directory": "vscode",
    "type": "git"
  },
  "files": [
    "/dependencies",
    "/media"
  ],
  "bugs": {
    "url": "https://github.com/karina-celis/codio-sui-maker/projects"
  },
  "version": "0.1.18",
  "license": "AGPL-3.0-only",
  "publisher": "techiemom",
  "author": {
    "name": "Techie Mom - KC"
  },
  "contributors": [
    "Elad Bogomolny",
    "Brady Dowling",
    "Robert Brisita"
  ],
  "icon": "media/logo.png",
  "engines": {
    "vscode": "^1.66.0"
  },
  "preview": true,
  "categories": [
    "Other"
  ],
  "keywords": [
    "Collaboration",
    "Record",
    "Share"
  ],
  "activationEvents": [
    "onCommand:codio.record.start",
    "onCommand:codio.play.start",
    "onView:codioMessages"
  ],
  "main": "./out/extension",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Codio configuration",
      "properties": {
        "codio.cursorColorDarkTheme": {
          "type": "string",
          "default": "rgb(81,80,82)",
          "description": "Codio dark theme cursor color."
        },
        "codio.cursorColorLightTheme": {
          "type": "string",
          "default": "rgb(255, 255, 255)",
          "description": "Codio light theme cursor color."
        }
      }
    },
    "commands": [
      {
        "command": "codio.play.forward",
        "category": "Codio",
        "title": "Play Forward",
        "icon": "$(debug-continue)"
      },
      {
        "command": "codio.play.goto",
        "category": "Codio",
        "title": "Player Go To...",
        "icon": "$(debug)"
      },
      {
        "command": "codio.play.pause",
        "category": "Codio",
        "title": "Play Pause",
        "icon": "$(debug-pause)"
      },
      {
        "command": "codio.play.resume",
        "category": "Codio",
        "title": "Play Resume",
        "icon": "$(debug-start)"
      },
      {
        "command": "codio.play.rewind",
        "category": "Codio",
        "title": "Play Rewind",
        "icon": "$(debug-reverse-continue)"
      },
      {
        "command": "codio.play.start",
        "category": "Codio",
        "title": "Play Start",
        "icon": "$(play)"
      },
      {
        "command": "codio.play.stop",
        "category": "Codio",
        "title": "Play Stop",
        "icon": "$(debug-stop)"
      },
      {
        "command": "codio.record.cancel",
        "category": "Codio",
        "title": "Record Cancel",
        "icon": "$(close)"
      },
      {
        "command": "codio.record.pause",
        "category": "Codio",
        "title": "Record Pause",
        "icon": "$(debug-pause)"
      },
      {
        "command": "codio.record.resume",
        "category": "Codio",
        "title": "Record Resume",
        "icon": "$(record)"
      },
      {
        "command": "codio.record.save",
        "category": "Codio",
        "title": "Record Save",
        "icon": "$(save)"
      },
      {
        "command": "codio.record.start",
        "category": "Codio",
        "title": "Record Start",
        "icon": "$(record)"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "codio.play.forward",
          "when": "!isRecording && isPlaying"
        },
        {
          "command": "codio.play.goto",
          "when": "!isRecording && isPlaying"
        },
        {
          "command": "codio.play.pause",
          "when": "!isRecording && isPlaying && !isPlayerPaused"
        },
        {
          "command": "codio.play.resume",
          "when": "!isRecording && isPlaying && isPlayerPaused"
        },
        {
          "command": "codio.play.rewind",
          "when": "!isRecording && isPlaying"
        },
        {
          "command": "codio.play.start",
          "when": "!isRecording && !isPlaying"
        },
        {
          "command": "codio.play.stop",
          "when": "!isRecording && isPlaying"
        },
        {
          "command": "codio.record.cancel",
          "when": "isRecording && !isPlaying"
        },
        {
          "command": "codio.record.pause",
          "when": "isRecording && !isRecordingPaused && !isPlaying"
        },
        {
          "command": "codio.record.resume",
          "when": "isRecording && isRecordingPaused && !isPlaying"
        },
        {
          "command": "codio.record.save",
          "when": "isRecording && !isPlaying"
        },
        {
          "command": "codio.record.start",
          "when": "!isRecording && !isPlaying"
        }
      ],
      "view/item/context": [
        {
          "command": "codio.play.start",
          "when": "viewItem == codio && !isPlaying",
          "group": "inline"
        },
        {
          "command": "codio.play.pause",
          "when": "viewItem == codio && !isRecording && isPlaying && !isPlayerPaused",
          "group": "inline"
        },
        {
          "command": "codio.play.resume",
          "when": "viewItem == codio && !isRecording && isPlaying && isPlayerPaused",
          "group": "inline"
        },
        {
          "command": "codio.play.goto",
          "when": "viewItem == debug && !isRecording && isPlaying",
          "group": "inline"
        }
      ],
      "view/title": [
        {
          "command": "codio.play.rewind",
          "when": "isPlaying && view == codioMessages",
          "group": "navigation@1"
        },
        {
          "command": "codio.play.resume",
          "when": "isPlaying && isPlayerPaused && view == codioMessages",
          "group": "navigation@2"
        },
        {
          "command": "codio.play.pause",
          "when": "isPlaying && !isPlayerPaused && view == codioMessages",
          "group": "navigation@2"
        },
        {
          "command": "codio.play.forward",
          "when": "isPlaying && view == codioMessages",
          "group": "navigation@3"
        },
        {
          "command": "codio.play.stop",
          "when": "isPlaying && view == codioMessages",
          "group": "navigation@4"
        },
        {
          "command": "codio.record.start",
          "when": "!isRecording && !isPlaying && view == codioMessages",
          "group": "navigation@1"
        },
        {
          "command": "codio.record.save",
          "when": "isRecording && !isPlaying && view == codioMessages",
          "group": "navigation@1"
        },
        {
          "command": "codio.record.pause",
          "when": "isRecording && !isRecordingPaused && !isPlaying && view == codioMessages",
          "group": "navigation@2"
        },
        {
          "command": "codio.record.resume",
          "when": "isRecording && isRecordingPaused && !isPlaying && view == codioMessages",
          "group": "navigation@2"
        },
        {
          "command": "codio.record.cancel",
          "when": "isRecording && !isPlaying && view == codioMessages",
          "group": "navigation@3"
        }
      ]
    },
    "views": {
      "explorer": [
        {
          "id": "codioMessages",
          "name": "Codios"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "codioMessages",
        "contents": "No codios found."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npx rimraf out && npx rimraf codio-*.vsix && npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run -S esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run -S esbuild-base -- --sourcemap --watch",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "lint": "eslint -c .eslintrc.js --ext .js,.ts ."
  },
  "devDependencies": {
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.40",
    "@types/vscode": "~1.66",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vscode/test-electron": "^2.1.4",
    "esbuild": "^0.14.44",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "vsce": "^2.9.1"
  },
  "dependencies": {
    "node-abort-controller": "^3.0.1",
    "subtitles-parser-vtt": "rbrisita/subtitles-parser#semver:~0.0.9"
  }
}
